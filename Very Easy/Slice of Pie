def equal_slices(total_slices, no_recipients, slices_each):
    total_slices_needed = no_recipients * slices_each
    return total_slices_needed <= total_slices

# Test cases
print(equal_slices(11, 5, 2))  # Output: True
print(equal_slices(11, 5, 3))  # Output: False
print(equal_slices(8, 3, 2))   # Output: True
print(equal_slices(8, 3, 3))   # Output: False
print(equal_slices(24, 12, 2)) # Output: True
