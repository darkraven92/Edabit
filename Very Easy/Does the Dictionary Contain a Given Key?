def has_key(dictionary, key):
    return key in dictionary

# Test cases
print(has_key({ "a": 44, "b": 45, "c": 46 }, "d"))  # Output: False
print(has_key({ "craves": True, "midnight": True, "snack": True }, "morning"))  # Output: False
print(has_key({ "pot": 1, "tot": 2, "not": 3 }, "not"))  # Output: True
