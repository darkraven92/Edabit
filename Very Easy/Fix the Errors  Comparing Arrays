def check_equals(lst1, lst2):
    if len(lst1) != len(lst2):
        return False

    for i in range(len(lst1)):
        if lst1[i] != lst2[i]:
            return False

    return True

# Test cases
print(check_equals([1, 2], [1, 3]))      # Output: False
print(check_equals([1, 2], [1, 2]))      # Output: True
print(check_equals([4, 5, 6], [4, 5, 6])) # Output: True
print(check_equals([4, 7, 6], [4, 5, 6])) # Output: False
print(check_equals([1, 12], [11, 2]))    # Output: False
