def value_at(lst, index):
    index = int(index)  # Convert the index to an integer using integer division
    return lst[index]

# Test cases
print(value_at([1, 2, 3, 4, 5, 6], 10 // 2))  # Output: 6
print(value_at([1, 2, 3, 4, 5, 6], 8.0 // 2))  # Output: 5
print(value_at([1, 2, 3, 4], 6.535355314 // 2))  # Output: 4
